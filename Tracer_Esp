local a=Vector3.new(10,10,10)local b=1;local c=false;local d=os.clock()game.StarterGui:SetCore("SendNotification",{Title="INFO",Text="ESP On",Icon="",Duration=5})local e=loadstring(game:HttpGet("https://pastebin.com/raw/ZLfeQchv"))()e:Toggle(true)e.Boxes=true;e.Names=false;e.Tracers=true;e.Players=false;e:AddObjectListener(workspace,{Name="soldier_model",Type="Model",Color=Color3.fromRGB(48,0,211),PrimaryPart=function(f)local g;repeat g=f:FindFirstChild("HumanoidRootPart")task.wait()until g;return g end,Validator=function(f)task.wait(1)if f:FindFirstChild("friendly_marker")then return false end;return true end,CustomName="",IsEnabled="enemy"})e.enemy=true;task.wait(1)for h,i in pairs(workspace:GetDescendants())do if i.Name=="soldier_model"and i:IsA("Model")and not i:FindFirstChild("friendly_marker")then local j=i:FindFirstChild("HumanoidRootPart").Position;for h,k in pairs(workspace:GetChildren())do if k:IsA("BasePart")then local l=(k.Position-j).Magnitude;if l<=5 then k.Transparency=b;k.Size=a end end end end end;local function m(n)task.wait(1)if n.Name=="soldier_model"and n:IsA("Model")and not n:FindFirstChild("friendly_marker")then if c then game.StarterGui:SetCore("SendNotification",{Title="INFO",Text="[Warning] New Enemy Spawned! Applied hitboxes.",Icon="",Duration=3})end;local j=n:FindFirstChild("HumanoidRootPart").Position;for h,k in pairs(workspace:GetChildren())do if k:IsA("BasePart")then local l=(k.Position-j).Magnitude;if l<=5 then k.Transparency=b;k.Size=a end end end end end;task.spawn(function()game.Workspace.DescendantAdded:Connect(m)end)local o=os.clock()local p=o-d;local q;if p<3 then q="fast"elseif p<5 then q="acceptable"else q="slow"end;game.StarterGui:SetCore("SendNotification",{Title="INFO",Text=string.format("Lethals loaded in %.2f seconds (%s loading)",p,q),Icon="",Duration=5})
